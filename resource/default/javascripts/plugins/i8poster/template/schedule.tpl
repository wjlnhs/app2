<div class="schedule-tab i8-form">
            <div class="schedule-body">
                <input class="schedule-name" placeholder="日程名称" />
                <div class="scheduleinput-box">
                    <input class="scheduleinput" placeholder="日程">
                    <i class="icon icon_select_gray"></i>
                </div>
                <div class="scheduleinput-box m-r0">
                    <input placeholder="2014-10-22">
                    <i class="icon icon_select_gray"></i>
                </div>
                <div class="scheduleinput-box">
                    <input class="scheduleinput" placeholder="17：30">
                    <i class="icon icon_select_gray"></i>
                </div>
                <div class="scheduleinput-box m-r0">
                    <input placeholder="17：50">
                    <i class="icon icon_select_gray"></i>
                </div>
                <div class="clear"></div>
                <div class="cate-row m-t15">
                    <span class="design-bg-icons3 app-checkbox checked v--4"></span>
                    <span class="cate repeat-cycle-txt">重复循环</span>
                </div>
                <div class="cate-row circulation">
                    <div class="schedule-time"><div class="in-bl w-70">日程时间</div>  16:00-16:30</div>
                    <div class="schedule-cycle">
                        <div class="in-bl w-70">循环周期</div>
                        <div class="in-bl w-85">
                            <span class="design-bg-icons3 app-radio checked"></span>
                            <span class="cate">每周</span>
                        </div>
                        <div class="in-bl w-85">
                            <span class="design-bg-icons3 app-radio checked"></span>
                            <span class="cate">每日</span>
                        </div>
                    </div>
                    <div class="scheduleinput-box">
                        <input placeholder="2014-10-22">
                    </div>
                    <div class="scheduleinput-box m-r0">
                        <input class="scheduleinput" placeholder="2014-10-22">
                    </div>
                    <div class="select-day">
                        <div>
                            <span class="design-bg-icons3 app-checkbox checked"></span>
                            <span>周一</span>
                        </div>
                        <div>
                            <span class="design-bg-icons3 app-checkbox"></span>
                            <span>周二</span>
                        </div>
                        <div>
                            <span class="design-bg-icons3 app-checkbox"></span>
                            <span>周三</span>
                        </div>
                        <div>
                            <span class="design-bg-icons3 app-checkbox"></span>
                            <span>周四</span>
                        </div>
                        <div>
                            <span class="design-bg-icons3 app-checkbox"></span>
                            <span>周五</span>
                        </div>
                        <div>
                            <span class="design-bg-icons3 app-checkbox"></span>
                            <span>周六</span>
                        </div>
                        <div>
                            <span class="design-bg-icons3 app-checkbox"></span>
                            <span>周日</span>
                        </div>
                    </div>
                </div>
                <div class="select-location">
                    <input class="schedule-location" placeholder="地点" />
                    <a class="icon icon-pin"></a>
                </div>
                <div class="select-people">
                    <input class="schedule-people"  />
                    <a class="icon icon-people-online"></a>
                    <div class="people-tiles">
                        <span class="tile-nor">蔡宝<i class="icon"></i></span>
                        <span class="tile-nor">蔡妹纸<i class="icon"></i></span>
                    </div>
                </div>
                <div class="prompt-time">
                    <div class="schedule-time-title">
                        到期时间：任务到期前
                    </div>
                    <input class="schedule-time-hour"  placeholder="15"/>
                    <div class="scheduleinput-box m-r0">
                        <input class="schedule-time-min" placeholder="分钟">
                        <i class="icon icon_select_gray"></i>
                    </div>
                    <textarea placeholder="详细内容"></textarea>
                </div>
            </div>
            <div class="schedule-footer">
                <ul class="functions-btns-left">
                    <li class="s-icon attachment-btn">附件</li>
                </ul>
                <a class="btn-blue-h32 schedule-sub">发布</a>
                <div class="clear"></div>
            </div>
        </div>